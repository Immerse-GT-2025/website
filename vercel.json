{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920],
    "domains": ["github.com"]
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 